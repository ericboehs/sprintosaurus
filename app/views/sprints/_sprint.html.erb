<div id="<%= dom_id sprint %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Iteration:</strong>
    <%= sprint.iteration_id %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Title:</strong>
    <%= sprint.title %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Start date:</strong>
    <%= sprint.start_date %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Duration:</strong>
    <%= sprint.duration %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Project:</strong>
    <%= sprint.project_id %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Created at:</strong>
    <%= sprint.created_at %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Updated at:</strong>
    <%= sprint.updated_at %>
  </p>

</div>
